{
  "name": "Game Assist",
  "name-ko-KR": "게임 도우미",
  "version": "1.0.0",
  "description": "Created to easily configure system power in Steam OS",
  "description-ko-KR": "스팀 OS에서 시스템 전력을 쉽게 설정하기 위해서 만들어졌습니다",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "createDist": "rsync -r --exclude \"src/\" --exclude \"__pycache__\" --exclude \"node_modules\" /plugin/ /out/"
  },
  "keywords": [
    "decky",
    "plugin",
    "steam-deck",
    "deck",
    "QoL",
    "bash",
    "PowerTool"
  ],
  "author": {
    "name": "화랑"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "16.14.0",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "rollup": "^2.77.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.24.5",
    "react-icons": "^4.4.0",
    "uuid": "^9.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
